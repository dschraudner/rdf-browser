{
    "manifest_version": 2,
    "name": "RDF Browser",
    "description": "Render RDF files as Turtle documents with clickable links",
    "version": "1.0.0",
    "browser_specific_settings": {
        "gecko": {
            "id": "rdf-browser@ti.rw.fau.de",
            "strict_min_version": "67.0"
        }
    },
    "background": {
        "scripts": [
            "src/intercept.js",
            "src/render-browser.js"
        ]
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}
