{
    "manifest_version": 2,
    "name": "RDF Browser",
    "description": "A Firefox AddOn that renders RDF files as Turtle documents with clickable links",
    "version": "1.0.0",
    "background": {
        "scripts": [
            "src/intercept.js",
            "src/render-browser.js"
        ]
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}
